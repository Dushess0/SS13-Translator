<!--
Title: PDA UI
Used In File(s): \code\game\objects\items\devices\PDA\PDA.dm
 -->

{{if data.owner}}
	<div class="item">
		<div class="itemLabelNarrow">
			 <b>Функции</b>:
        </div>
        <div class="itemContent">
			<!--{{:helper.link('Обновить', 'refresh', {'choice' : "Refresh"}, null, 'fixedLeft')}}-->
			{{:helper.link('Закрыть', 'gear', {'choice' : "Close"}, null, 'fixedLeft')}}
			{{if data.idInserted}} {{:helper.link('Обновить информацию ПДА', 'eject', {'choice' : "UpdateInfo"}, null, 'fixedLeftWide')}} {{/if}}
			{{if data.mode != 0}} {{:helper.link('Назад', 'arrowreturn-1-w', {'choice' : "Return"}, null, 'fixedLeft')}} {{/if}}
		</div>
	</div>
	<br>
	<div class="item">
		<div class="itemLabelNarrow">
			<b>Станционное время</b>:
		</div>
		<div class="itemContent">
			<span class="average">{{:data.stationTime}}&emsp;[{{:data.securityLevel}}]</span>
		</div>
	</div>
    {{if data.shuttle_eta}}
        <div class="item">
            <div class="itemLabelNarrow">
                <b>Время до шаттла</b>:
            </div>
            <div class="itemContent">
                <span class="average">{{:data.shuttle_eta}}</span>
            </div>
        </div>
    {{/if}}
    <br>


	{{if data.mode == 0}} <!-- Main Menu -->
        <div class="item">
            <div class="itemLabelNarrow">
                Владелец:
            </div>
            <div class="itemContent">
                <span class="average">{{:data.owner}}, {{:data.ownjob}}</span>
            </div>
        </div>
        <br>
        <div class="item">
            <div class="itemLabelNarrow">
                ID:
            </div>
            <div class="itemContent">
                {{:helper.link(data.idLink, 'eject', {'choice' : "Authenticate"}, data.idInserted ? null : 'disabled', data.idInserted ? 'floatright' : 'fixedLeft')}}
            </div>
        </div>
        <br>
        <div class="item">
            <div class="itemLabelNarrow">
                    <b>Cartridge</b>:
            </div>
            <div class="itemContent">
                {{if data.cart_loaded==1}}
                    {{:helper.link(data.cartridge.name, 'eject', {'choice' : "Eject"},null,null)}}
                {{else}}
                    {{:helper.link('None', 'eject', {'choice' : "Eject"},'disabled',null)}}
                {{/if}}
            </div>
        </div>
        <div class="item">
            <H2>Функции</H2>
        </div>
        <div class="item">
            <div class="itemLabelNarrow">
                <b>Общее</b>:
            </div>
            <div class="itemContent">
                {{:helper.link('Записная книжка', 'note', {'choice' : "1"}, null, 'fixedLeftWide')}}
                {{:helper.link('Сообщения', data.new_Message ? 'mail-closed' : 'mail-open', {'choice' : "2"}, null, 'fixedLeftWide')}}
                {{:helper.link('Список экипажа', 'contact', {'choice' : "41"}, null, 'fixedLeftWide')}}
				<!-- {{:helper.link('Новости', data.new_News ? 'mail-closed' : 'mail-open', {'choice' : "6"}, null, 'fixedLeftWide')}} -->
				{{:helper.link('Управление финансами', 'suitcase', {'choice' : "Finance"}, null, 'fixedLeftWide')}}
            </div>
        </div>
        <br>
        {{if data.cartridge}}
            {{if data.cartridge.access.access_clown == 1}}
                <div class="item">
                    <div class="itemLabelNarrow">
                        <b>Клоун</b>:
                    </div>
                    <div class="itemContent">
                        {{:helper.link('Синтезатор ХОНКа', 'gear', {'choice' : "Honk"}, null, 'fixedLeftWide')}}
                    </div>
                </div>
                <br>
            {{/if}}
            {{if data.cartridge.access.access_engine == 1}}
                <div class="item">
                    <div class="itemLabelNarrow">
                        <b>Инженерное</b>:
                    </div>
                    <div class="itemContent">
                        {{:helper.link('Монитор энергосети', 'alert', {'choice' : "43"}, null, 'fixedLeftWide')}}
                    </div>
                </div>
                 <br>
            {{/if}}
            {{if data.cartridge.access.access_medical == 1}}
                <div class="item">
                    <div class="itemLabelNarrow">
                            <b>Медицинское</b>:
                    </div>
                    <div class="itemContent">
                            {{:helper.link('Медицинские записи', 'gear', {'choice' : "44"}, null, 'fixedLeftWide')}}
                            {{:helper.link(data.scanmode == 1 ? 'Выключить медицинский сканнер' : 'Включить медицинский сканнер', 'gear', {'choice' : "Medical Scan"}, null , 'fixedLeftWide')}}
                    </div>
                </div>
                <br>
            {{/if}}
            {{if data.cartridge.access.access_security == 1}}
                <div class="item">
                    <div class="itemLabelNarrow">
                            <b>Служба Безопасности</b>:
                    </div>
                    <div class="itemContent">
                        {{:helper.link('Криминальные записи', 'gear', {'choice' : "45"}, null, 'fixedLeftWide')}}
                        {{if data.cartridge.radio ==1}} {{:helper.link('Управление дронами охраны', 'gear', {'choice' : "46"}, null, 'fixedLeftWide')}} {{/if}}
                        <br>
                    </div>
                </div>
                <br>
            {{/if}}
            {{if data.cartridge.access.access_quartermaster == 1}}
                <div class="item">
                    <div class="itemLabelNarrow">
                        <b>Снабжение</b>:
                    </div>
                    <div class="itemContent">
                        {{:helper.link('Записи снабжения', 'gear', {'choice' : "47"}, null, 'fixedLeftWide')}}
                        {{if data.cartridge.radio == 3}} {{:helper.link('Управление дронами доставки', 'gear', {'choice' : "48"}, null, 'fixedLeftWide')}} {{/if}}
                         <br>
                    </div>
                </div>
                <br>
            {{/if}}
        {{/if}}
        <br>
        <div class="item">
            <div class = "itemLabelNarrow">
                <b>Разное</b>:
            </div>
            <div class = "itemContent">
                {{if data.cartridge}}
                    {{if data.cartridge.access.access_status_display == 1}}
                        {{:helper.link('Управление экранами статуса', 'gear', {'choice' : "42"}, null, 'fixedLeftWide')}}
                    {{/if}}
                    {{if data.cartridge.access.access_janitor==1}}
                        {{:helper.link('Указатель вещей уборщика', 'gear', {'choice' : "49"}, null, 'fixedLeftWide')}}
                    {{/if}}
                    {{if data.cartridge.radio == 2}}
                        {{:helper.link('Сигналер', 'gear', {'choice' : "40"}, null, 'fixedLeftWide')}}
                    {{/if}}
                    {{if data.cartridge.access.access_reagent_scanner==1}}
                        {{:helper.link(data.scanmode == 3 ? 'Выключить Сканнер Реагентов' : 'Включить Сканнер Реагентов', 'gear', {'choice' : "Сканирование реагентов"}, null, 'fixedLeftWider')}}
                    {{/if}}
                    {{if data.cartridge.access.access_engine==1}}
                        {{:helper.link(data.scanmode == 4 ? 'Выключить счётчик Гейгера' : 'Включить счётчик Гейгера', 'gear', {'choice' : "Счётчик Гейгера"}, null, 'fixedLeftWider')}}
                    {{/if}}
                    {{if data.cartridge.access.access_atmos==1}}
                        {{:helper.link(data.scanmode == 5 ? 'Выключить Сканнер Газов' : 'Включить Сканнер Газов', 'gear', {'choice' : "Сканирование газов"}, null, 'fixedLeftWide')}}
                    {{/if}}
                    {{if data.cartridge.access.access_remote_door==1}}
                        {{:helper.link('Переключить дверь', 'gear', {'choice' : "Переключить дверь"}, null, 'fixedLeftWide')}}
                    {{/if}}
                {{/if}}
                {{:helper.link('Atmospheric Scan', 'gear', {'choice' : "3"}, null, 'fixedLeftWide')}}
                {{:helper.link(data.fon==1 ? 'Выключить Фонарик' : 'Включить Фонарик', 'lightbulb', {'choice' : "Фонарик"}, null,'fixedLeftWide')}}
            </div>
        </div>
        {{if data.pai}}
            <div class="item">
                <div class = "itemLabelNarrow">
                    <b>PAI Utilities</b>:
                </div>
                <div class = "itemContent">
                    {{:helper.link('Настройка', 'gear', {'choice' : "pai", 'option' : "1"}, null, 'fixedLeft')}}
                    {{:helper.link('Вынять pAI', 'eject', {'choice' : "pai", 'option' : "2"}, null, 'fixedLeft')}}
                </div>
            </div>
        {{/if}}


	{{else data.mode == 1}} <!-- Notekeeper -->
        <div class="item">
            <div class="itemLabel">
                <b>Записи</b>:
            </div>
        </div>
        <div class="statusDisplayRecords">
            <div class="item">
                <div class="itemContent" style="width: 100%;">
                        <span class="average">{{:data.note}}</span>
                </div>
            </div>
        </div>
        <div class="item">
            <div class="itemLabel">
                {{:helper.link('Редактировать записи', 'gear', {'choice' : "Редактировать"}, null, 'fixedLeft')}}
            </div>
        </div>


    {{else data.mode == 2}} <!-- Messenger -->
        <H2>Космо-Мессенджер V4.0.1</H2>
        <div class="item">
            <div class="itemLabelNarrow">
                <b>Функции мессенджера</b>:
            </div>
            <div class ="itemContent">
                {{:helper.link(data.message_silent==1 ? 'Звонок: Выключен' : 'Звонок: Включен', data.message_silent==1 ? 'volume-off' : 'volume-on', {'choice' : "Toggle Ringer"}, null, 'fixedLeftWide')}}
                {{:helper.link(data.toff==1 ? 'Мессенджер: Выключен' : 'Мессенджер: Включен',data.toff==1 ? 'close':'check', {'choice' : "Toggle Messenger"}, null, 'fixedLeftWide')}}
                {{:helper.link('Установить звонок', 'comment', {'choice' : "Ringtone"}, null, 'fixedLeftWide')}}
                {{:helper.link('Удалить все переписки', 'trash', {'choice' : "Clear", 'option' : "All"}, null, 'fixedLeftWider')}}
            </div>
        </div>
        {{if data.toff == 0}}
            <br><br>
            {{if data.cartridge}}
                {{if data.cartridge.charges}}
                    <div class="item">
                        <b>{{:data.cartridge.charges}}
                        {{if data.cartridge.access.access_detonate_pda}} зарядов для подрыва осталось. {{/if}}
                        {{if data.cartridge.access.access_clown || data.cartridge.access.access_mime}} вирусных файлов осталось. {{/if}}
                        </b>
                        <br><br>
                    </div>
                {{/if}}
            {{/if}}

            {{if data.pda_count == 0}}
                <b>Не обнаружено других ПДА</b>
            {{else}}
                <H3>Текущие Переписки</H3>
                {{for data.convopdas}}
                    <div class="item">
                        {{:helper.link(value.Name, 'circle-arrow-s', {'choice' : "Выбрать Переписку", 'convo' : value.Reference } , null, value.fixedLeftWider)}}
                        {{if data.cartridge}}
                            {{if data.cartridge.access.access_detonate_pda && value.Detonate}}
                                {{:helper.link('*Подорвать*', 'radiation', {'choice' : "Подорвать", 'target' : value.Reference}, null, 'fixedLeft')}}
                            {{/if}}
                            {{if data.cartridge.access.access_clown}}
                                {{:helper.link('*Отправить Вирус*', 'star', {'choice' : "Отправить ХОНК", 'target' : value.Reference}, null, 'fixedLeft')}}
                            {{/if}}
                            {{if data.cartridge.access.access_mime}}
                                {{:helper.link('*Отправить Вирус*', 'circle-arrow-s', {'choice' : "Отправить Silence", 'target' : value.Reference}, null, 'fixedLeft')}}
                            {{/if}}
                        {{/if}}
                    </div>
                {{/for}}
                <H3>Other PDAs</H3>
                {{for data.pdas}}
                    <div class="item">
                        {{:helper.link(value.Name, 'circle-arrow-s', {'choice' : "Сообщение", 'target' : value.Reference}, null, value.fixedLeftWider)}}
                        {{if data.cartridge}}
                            {{if data.cartridge.access.access_detonate_pda && value.Detonate}} {{:helper.link('*Detonate*', 'radiation', {'choice' : "Detonate", 'target' : value.Reference}, null, 'fixedLeft')}}  {{/if}}
                            {{if data.cartridge.access.access_clown}} {{:helper.link('*Отправить Вирус*', 'star', {'choice' : "Отправить ХОНК", 'target' : value.Reference}, null, 'fixedLeft')}}  {{/if}}
                            {{if data.cartridge.access.access_mime}}  {{:helper.link('*Отправить Вирус*', 'circle-arrow-s', {'choice' : "Отправить ТИШИНКУ", 'target' : value.Reference}, null, 'fixedLeft')}}  {{/if}}
                        {{/if}}
                    </div>
                {{/for}}
            {{/if}}
        {{/if}}


	{{else data.mode == 21}} <!-- Messenger -->
	    <H2>Космо-Мессенджер V4.0.1</H2>
        <div class="item">
            <div class="itemLabel">
                <b>Функции мессенджера</b>:
            </div>
            <div class ="itemContent">
                {{:helper.link('Удалить переписку', 'trash', {'choice' : "Очистить", 'option' : "Convo"}, null, 'fixedLeftWide')}}
            </div>
        </div>
        <br>
        <br>
        <H3>Переписки с:&nbsp;<span class="average">{{:data.convo_name}}&nbsp;({{:data.convo_job}})</span></H3>
        <div class="statusDisplay" style="overflow: auto;">
            <div class="item">
                <div class="itemContent" style="width: 100%;">
                    {{for data.messages}}
                        {{if data.active_conversation == value.target}}
                            {{if value.sent==0}}
                                <span class="average"><B>Они</B>: {{:value.message}}</span><br>
                            {{else}}
                                <span class="good"><B>Вы</B>: {{:value.message}}</span><br>
                            {{/if}}
                        {{/if}}
                    {{/for}}
                </div>
            </div>
        </div>
        {{:helper.link('Ответить', 'comment', {'choice' : "Написать", 'target': data.active_conversation}, null, 'fixedLeft')}}


	{{else data.mode== 41}} <!-- Manifest uses cached data so we only use nanowriter when shit changes.-->
	    <div class="item">
            <center><table class="pmon"><tbody>
                {{if data.cached.heads.length}}
                    <tr><th colspan="4" class="command">Командный состав</th></tr>
                    {{for data.cached["heads"]}}
                        {{if value.rank == "Captain"}}
                            <tr><td>{{:helper.link('Отправить Деньги', null, {'choice' : "Отправить Деньги", 'account' : value.account}, null, null)}}</td>
                            <td><span class="good">{{:value.name}}</span></td><td><span class="good">{{:value.rank}}</span></td><td><span class="good">{{:value.active}}</span></td></tr>
                        {{else}}
                            <tr><td>{{:helper.link('Отправить Деньги', null, {'choice' : "Отправить Деньги", 'account' : value.account}, null, null)}}</td>
                            <td><span class="average">{{:value.name}}</span></td><td><span class="average">{{:value.rank}}</span></td><td><span class="average">{{:value.active}}</span></td></tr>
                        {{/if}}
                    {{/for}}
                {{/if}}
                {{if data.cached.sec.length}}
                    <tr><th colspan="4" class="sec">Служба Безопасности</th></tr>
                    {{for data.cached["sec"]}}
                        {{if value.rank == "Head of Security"}}
                            <tr><td>{{:helper.link('Отправить Деньги', null, {'choice' : "Отправить Деньги", 'account' : value.account}, null, null)}}</td>
                            <td><span class="good">{{:value.name}}</span></td><td><span class="good">{{:value.rank}}</span></td><td><span class="good">{{:value.active}}</span></td></tr>
                        {{else}}
                            <tr><td>{{:helper.link('Отправить Деньги', null, {'choice' : "Отправить Деньги", 'account' : value.account}, null, null)}}</td>
                            <td><span class="average">{{:value.name}}</span></td><td><span class="average">{{:value.rank}}</span></td><td><span class="average">{{:value.active}}</span></td></tr>
                        {{/if}}
                    {{/for}}
                {{/if}}
                {{if data.cached.eng.length}}
                    <tr><th colspan="4" class="eng">Инженеры</th></tr>
                    {{for data.cached["eng"]}}
                        {{if value.rank == "Chief Engineer"}}
                            <tr><td>{{:helper.link('Отправить Деньги', null, {'choice' : "Отправить Деньги", 'account' : value.account}, null, null)}}</td>
                            <td><span class="good">{{:value.name}}</span></td><td><span class="good">{{:value.rank}}</span></td><td><span class="good">{{:value.active}}</span></td></tr>
                        {{else}}
                            <tr><td>{{:helper.link('Отправить Деньги', null, {'choice' : "Отправить Деньги", 'account' : value.account}, null, null)}}</td>
                            <td><span class="average">{{:value.name}}</span></td><td><span class="average">{{:value.rank}}</span></td><td><span class="average">{{:value.active}}</span></td></tr>
                        {{/if}}
                    {{/for}}
                {{/if}}
                {{if data.cached.med.length}}
                    <tr><th colspan="4" class="med">Медики</th></tr>
                    {{for data.cached["med"]}}
                        {{if value.rank == "Chief Medical Officer"}}
                            <tr><td>{{:helper.link('Отправить Деньги', null, {'choice' : "Отправить Деньги", 'account' : value.account}, null, null)}}</td>
                            <td><span class="good">{{:value.name}}</span></td><td><span class="good">{{:value.rank}}</span></td><td><span class="good">{{:value.active}}</span></td></tr>
                        {{else}}
                            <tr><td>{{:helper.link('Отправить Деньги', null, {'choice' : "Отправить Деньги", 'account' : value.account}, null, null)}}</td>
                            <td><span class="average">{{:value.name}}</span></td><td><span class="average">{{:value.rank}}</span></td><td><span class="average">{{:value.active}}</span></td></tr>
                        {{/if}}
                    {{/for}}
                {{/if}}
                {{if data.cached.sci.length}}
                    <tr><th colspan="4" class="sci">Ученые</th></tr>
                    {{for data.cached["sci"]}}
                        {{if value.rank == "Research Director"}}
                            <tr><td>{{:helper.link('Отправить Деньги', null, {'choice' : "Отправить Деньги", 'account' : value.account}, null, null)}}</td>
                            <td><span class="good">{{:value.name}}</span></td><td><span class="good">{{:value.rank}}</span></td><td><span class="good">{{:value.active}}</span></td></tr>
                        {{else}}
                            <tr><td>{{:helper.link('Отправить Деньги', null, {'choice' : "Отправить Деньги", 'account' : value.account}, null, null)}}</td>
                            <td><span class="average">{{:value.name}}</span></td><td><span class="average">{{:value.rank}}</span></td><td><span class="average">{{:value.active}}</span></td></tr>
                        {{/if}}
                    {{/for}}
                {{/if}}
                {{if data.cached.civ.length}}
                    <tr><th colspan="4" class="civ">Гражданские</th></tr>
                    {{for data.cached["civ"]}}
                        {{if value.rank == "Head of Personnel"}}
                            <tr><td>{{:helper.link('Отправить Деньги', null, {'choice' : "Отправить Деньги", 'account' : value.account}, null, null)}}</td>
                            <td><span class="good">{{:value.name}}</span></td><td><span class="good">{{:value.rank}}</span></td><td><span class="good">{{:value.active}}</span></td></tr>
                        {{else}}
                            <tr><td>{{:helper.link('Отправить Деньги', null, {'choice' : "Отправить Деньги", 'account' : value.account}, null, null)}}</td>
                            <td><span class="average">{{:value.name}}</span></td><td><span class="average">{{:value.rank}}</span></td><td><span class="average">{{:value.active}}</span></td></tr>
                        {{/if}}
                    {{/for}}
                {{/if}}
                {{if data.cached.misc.length}}
                    <tr><th colspan="4" class="misc">Разное</th></tr>
                    {{for data.cached["misc"]}}
                        <tr><td>{{:helper.link('Отправить Деньги', null, {'choice' : "Отправить Деньги", 'account' : value.account}, null, null)}}</td>
                        <td><span class="average">{{:value.name}}</span></td><td><span class="average">{{:value.rank}}</span></td><td><span class="average">{{:value.active}}</span></td></tr>
                    {{/for}}
                {{/if}}
            </tbody></table></center>
        </div>


	{{else data.mode == 3}}
        <H2>Атмосферное сканирование</H2>
        <div class="statusDisplay" style="height: 250px; overflow: auto;">
            <div class="item">
                {{if data.aircontents.reading == 1}}
                    <div class="itemLabel">
                        Давление:
                    </div>
                    <div class = "itemContent">
				        {{:helper.string('<span class="{0}">{1} кПа</span>', data.aircontents.pressure < 80 || data.aircontents.pressure > 120 ? 'bad' : data.aircontents.pressure < 95 || data.aircontents.pressure > 110 ? 'average' : 'good' , data.aircontents.pressure)}}
                    </div>
                    <div class="itemLabel">
                        Температура:
                    </div>
                    <div class = "itemContent">
				        {{:helper.string('<span class="{0}">{1} &deg;C</span>', data.aircontents.temp < 5 || data.aircontents.temp > 35 ? 'bad' : data.aircontents.temp < 15 || data.aircontents.temp > 25 ? 'average' : 'good' , data.aircontents.temp)}}
                    </div>
                    <br>
                    <div class="itemLabel">
                        Кислород:
                    </div>
                    <div class = "itemContent">
                        {{:helper.string('<span class="{0}">{1}%</span>', data.aircontents.oxygen < 17 ? 'bad' : data.aircontents.oxygen < 19 ? 'average' : 'good' , data.aircontents.oxygen)}}
                    </div>
                    <div class="itemLabel">
                        Азот:
                    </div>
                    <div class = "itemContent">
                        {{:helper.string('<span class="{0}">{1}%</span>', data.aircontents.nitrogen > 82 ? 'bad' : data.aircontents.nitrogen > 80 ? 'average' : 'good' , data.aircontents.nitrogen)}}
                    </div>
                    <div class="itemLabel">
                        Углекислый газ:
                    </div>
                    <div class = "itemContent">
                        {{:helper.string('<span class="{0}">{1}%</span>', data.aircontents.carbon_dioxide > 5 ? 'bad' : 'good' , data.aircontents.carbon_dioxide)}}
                    </div>
                    <div class="itemLabel">
                        Форон:
                    </div>
                    <div class = "itemContent">
                        {{:helper.string('<span class="{0}">{1}%</span>', data.aircontents.phoron  > 0 ? 'bad' : 'good' , data.aircontents.phoron)}}

                    </div>
                    {{if data.aircontents.other > 0}}
                        <div class="itemLabel">
                            Неизвестно:
                        </div>
                        <div class = "itemContent">
                            <span class="bad">{{:data.aircontents.other}}%</span>
                        </div>
                    {{/if}}
                {{else}}
                    <div class="itemContent" style="width: 100%;">
                        <span class="average"><b>Не удалось просканировать воздух</b></span>
                    </div>
                {{/if}}
            </div>
        </div>


	{{else data.mode == 40}} <!-- Cartridge: Signaler -->
        <H2>Удаленная сигнальная система</H2>
        <div class="item">
            <div class="itemLabel">
                <b>Частота</b>:
            </div>
            <div class="itemContent">
                {{:data.records.signal_freq}}
                <br>
